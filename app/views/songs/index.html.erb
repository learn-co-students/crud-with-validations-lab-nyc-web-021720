<h1>All of the Songs</h1>


<table>
    <tbody>
  
        <th>Title</th>
        
        <th> Artist</th>
 

           <% @songs.each do |song| %>
                <tr>
                    <td><%= song.title %></td> 

                    <td><%= song.artist_name %></td> 
                    
                    <td><%= link_to "Edit", edit_song_path(song) %> / <%= link_to "Delete", song_path(song), method: :delete %></td>
                </tr>
            <% end %>
    </tbody>
</table>







